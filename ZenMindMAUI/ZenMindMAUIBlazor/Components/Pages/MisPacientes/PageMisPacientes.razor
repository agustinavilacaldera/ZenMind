@page "/misPacientes"
@inject Controllers.ModelController model;
@inject NavigationManager navman

<TituloPagina Titulo="Mis Pacientes" href="/home/doctor" />

<ListaMedicoPaciente MedicoId="medicos.Id" SelectedPaciente="seleccionarPaciente"/>
@code {
  private Medicos medicos;
  protected override void OnInitialized()
  {
    medicos = model.CargarMedicoPorUserId(model.User.UserName);
  }
  private void seleccionarPaciente(int pId)
  {
    navman.NavigateTo($"/detallePaciente/{pId}");
  }

}
